{
  "name": "internxt-backup",
  "version": "0.4.0",
  "description": "A simple, fast, and efficient tool for backing up files to Internxt Drive",
  "type": "module",
  "main": "index.ts",
  "bin": {
    "internxt-backup": "./dist/bin/index.js"
  },
  "scripts": {
    "start": "bun index.ts",
    "test": "bun test",
    "build": "bun build --minify --target bun --sourcemap=linked ./index.ts --outdir ./dist/bin",
    "prepublishOnly": "bun test && bun run build"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "index.ts",
    "src/**/*.ts",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "internxt",
    "upload",
    "file-transfer",
    "cli",
    "backup",
    "cloud-storage",
    "file-sync",
    "encryption"
  ],
  "author": "ngarate",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "bun-types": "^1.2.8",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "croner": "^9.0.0"
  }
}
